{
	"name": "mh-photography.com",
	"type": "module",
	"version": "2.3.0-alpha.5",
	"license": "LICENSE",
	"private": true,
	"description": "MH Photography",
	"keywords": [
		"website",
		"photography",
		"personal-website",
		"portfolio-website"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Th3S4mur41/mh-photography.com.git"
	},
	"homepage": "https://mh-photography.com",
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.22.9",
		"@babel/preset-env": "^7.22.9",
		"@commitlint/cli": "^17.6.7",
		"@commitlint/config-conventional": "^17.6.7",
		"@hint/formatter-codeframe": "^3.1.35",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@squoosh/cli": "^0.7.3",
		"@typescript-eslint/eslint-plugin": "^6.2.0",
		"@typescript-eslint/parser": "^6.2.0",
		"autoprefixer": "^10.4.14",
		"babel-loader": "^9.1.3",
		"browserslist": "^4.21.9",
		"concurrently": "^8.2.0",
		"cssnano": "^6.0.1",
		"cssremedy": "^0.1.0-beta.2",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.9.0",
		"eslint-plugin-compat": "^4.1.4",
		"hint": "^7.1.10",
		"html-loader": "^4.2.0",
		"husky": "^8.0.3",
		"image-size": "^1.0.2",
		"postcss": "^8.4.27",
		"postcss-loader": "^7.3.3",
		"postcss-nesting": "^12.0.0",
		"postcss-preset-env": "^9.1.0",
		"prettier": "^3.0.0",
		"semantic-release": "^21.0.7",
		"sharp": "^0.32.4",
		"stylelint": "^15.10.2",
		"stylelint-config-property-sort-order-smacss": "^9.1.0",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-order": "^6.0.3",
		"stylelint-use-logical-spec": "^5.0.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.7",
		"vite-plugin-handlebars": "^1.6.0",
		"wait-on": "^7.0.1"
	},
	"optionalDependencies": {
		"lint-staged": "^13.2.3"
	},
	"scripts": {
		"start": "astro dev",
		"start:prod": "astro preview",
		"build": "astro build",
		"build:img": "node src/utils/sharp.js",
		"lint": "concurrently 'npm:lint:*(!fix)'",
		"lint:fix": "concurrently -m 1 'npm:lint:*:fix'",
		"lint:js": "eslint \"src/**/*.{js,mjs,cjs,ts}\" --no-error-on-unmatched-pattern --ignore-path .gitignore",
		"lint:js:fix": "eslint \"src/**/*.{js,mjs,cjs,ts}\" --no-error-on-unmatched-pattern --ignore-path .gitignore --fix",
		"lint:css": "stylelint \"src/**/*.{css,less,sass,scss}\" --aei --ignore-path .gitignore",
		"lint:css:fix": "stylelint \"src/**/*.{css,less,sass,scss}\" --aei --ignore-path .gitignore --fix",
		"lint:style": "prettier --check --ignore-unknown .",
		"lint:style:fix": "prettier --write --ignore-unknown .",
		"hint": "wait-on -t 300000 http://localhost:3000/ && hint http://localhost:3000/",
		"test": "concurrently 'npm:test:*'",
		"test:hint": "concurrently \"npm:start:prod\" \"npm:hint\" --kill-others  --s first",
		"release": "semantic-release",
		"prepare": "husky install",
		"git:pre-commit": "lint-staged",
		"git:pre-push": "npm run build && npm run test"
	},
	"dependencies": {
		"@fontsource/gfs-didot": "^5.0.5",
		"@fontsource/titillium-web": "^5.0.5",
		"astro": "^2.9.4"
	}
}

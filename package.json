{
	"name": "mh-photography.com",
	"type": "module",
	"version": "2.7.3",
	"license": "LICENSE",
	"private": true,
	"description": "MH Photography",
	"keywords": [
		"website",
		"photography",
		"personal-website",
		"portfolio-website"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Th3S4mur41/mh-photography.com.git"
	},
	"homepage": "https://mh-photography.com",
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.24.5",
		"@babel/preset-env": "^7.24.5",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@csstools/postcss-oklab-function": "^3.0.10",
		"@hint/formatter-codeframe": "^3.1.35",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@typescript-eslint/eslint-plugin": "^7.8.0",
		"@typescript-eslint/parser": "^7.8.0",
		"autoprefixer": "^10.4.19",
		"babel-loader": "^9.1.3",
		"browserslist": "^4.22.1",
		"concurrently": "^8.2.2",
		"cssnano": "^7.0.1",
		"cssremedy": "^0.1.0-beta.2",
		"eslint": "^8.57.0",
		"eslint-plugin-compat": "^4.2.0",
		"hint": "^7.1.11",
		"husky": "^9.0.11",
		"image-size": "^1.1.1",
		"postcss": "^8.4.35",
		"postcss-loader": "^8.1.1",
		"postcss-nesting": "^12.0.1",
		"postcss-preset-env": "^9.5.11",
		"prettier": "^3.2.5",
		"semantic-release": "^23.0.8",
		"sharp": "^0.33.3",
		"stylelint": "^16.5.0",
		"stylelint-config-property-sort-order-smacss": "^10.0.0",
		"stylelint-config-standard": "^36.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-use-logical-spec": "^5.0.1",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"wait-on": "^7.2.0"
	},
	"optionalDependencies": {
		"lint-staged": "^15.2.2"
	},
	"scripts": {
		"start": "astro dev",
		"start:prod": "astro preview",
		"build": "astro build",
		"build:img": "node src/utils/sharp.cjs",
		"lint": "concurrently 'npm:lint:*(!fix)'",
		"lint:fix": "concurrently -m 1 'npm:lint:*:fix'",
		"lint:js": "eslint \"src/**/*.{js,mjs,cjs,ts}\" --no-error-on-unmatched-pattern --ignore-path .gitignore",
		"lint:js:fix": "eslint \"src/**/*.{js,mjs,cjs,ts}\" --no-error-on-unmatched-pattern --ignore-path .gitignore --fix",
		"lint:css": "stylelint \"src/**/*.{css,less,sass,scss}\" --aei --ignore-path .gitignore",
		"lint:css:fix": "stylelint \"src/**/*.{css,less,sass,scss}\" --aei --ignore-path .gitignore --fix",
		"lint:style": "prettier --check --ignore-unknown .",
		"lint:style:fix": "prettier --write --ignore-unknown .",
		"hint": "wait-on -t 300000 http://localhost:4321/ && hint http://localhost:4321/",
		"test": "concurrently 'npm:test:*'",
		"test:hint": "concurrently \"npm:start:prod\" \"npm:hint\" --kill-others  --s first",
		"release": "semantic-release",
		"prepare": "husky install",
		"git:pre-commit": "lint-staged",
		"git:pre-push": "npm run build && npm run test"
	},
	"dependencies": {
		"@astrojs/sitemap": "^3.1.4",
		"@fontsource/caveat": "^5.0.18",
		"@fontsource/gfs-didot": "^5.0.14",
		"@fontsource/titillium-web": "^5.0.20",
		"astro": "^4.8.2"
	}
}

{
	"name": "mh-photography.com",
	"version": "1.0.0-alpha.5",
	"license": "LICENSE",
	"private": true,
	"description": "MH Photography",
	"keywords": [
		"website",
		"photography",
		"personal-website",
		"portfolio-website"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Th3S4mur41/mh-photography.com.git"
	},
	"homepage": "https://mh-photography.com",
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.20.2",
		"@babel/preset-env": "^7.20.2",
		"@commitlint/cli": "^17.2.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@hint/formatter-codeframe": "^3.1.33",
		"@semantic-release/changelog": "^6.0.1",
		"@semantic-release/git": "^10.0.1",
		"@squoosh/cli": "^0.7.2",
		"@typescript-eslint/eslint-plugin": "^5.43.0",
		"@typescript-eslint/parser": "^5.44.0",
		"autoprefixer": "^10.4.13",
		"babel-loader": "^9.1.0",
		"browserslist": "^4.21.4",
		"concurrently": "^7.6.0",
		"cssnano": "^5.1.14",
		"cssremedy": "^0.1.0-beta.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-compat": "^4.0.2",
		"hint": "^7.1.2",
		"html-loader": "^4.2.0",
		"husky": "^8.0.2",
		"image-size": "^1.0.2",
		"postcss": "^8.4.19",
		"postcss-loader": "^7.0.1",
		"postcss-nesting": "^10.2.0",
		"postcss-preset-env": "^7.8.3",
		"prettier": "^2.7.1",
		"semantic-release": "^19.0.5",
		"sharp": "^0.31.2",
		"stylelint": "^14.15.0",
		"stylelint-config-prettier": "^9.0.4",
		"stylelint-config-property-sort-order-smacss": "^9.0.0",
		"stylelint-config-standard": "^29.0.0",
		"stylelint-order": "^5.0.0",
		"stylelint-use-logical-spec": "^4.1.0",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"vite-plugin-handlebars": "^1.6.0",
		"wait-on": "^6.0.1"
	},
	"optionalDependencies": {
		"lint-staged": "^13.0.3",
		"pretty-quick": "^3.1.3"
	},
	"scripts": {
		"start": "vite",
		"start:prod": "vite preview",
		"build": "vite build",
		"build:watch": "tsc --watch",
		"build:img": "node tools/sharp.js",
		"lint": "concurrently 'npm:lint:*(!fix)'",
		"lint:fix": "concurrently -m 1 'npm:lint:*:fix'",
		"lint:js": "eslint \"src/**/*.{js,mjs,ts}\" --no-error-on-unmatched-pattern --ignore-path .gitignore",
		"lint:js:fix": "eslint \"src/**/*.{js,mjs,ts}\" --no-error-on-unmatched-pattern --ignore-path .gitignore --fix",
		"lint:css": "stylelint \"src/**/*.{css, less, sass, scss}\" --aei --ignore-path .gitignore",
		"lint:css:fix": "stylelint \"src/**/*.{css,less,sass,scss}\" --aei --ignore-path .gitignore --fix",
		"lint:style": "prettier --check --ignore-unknown .",
		"lint:style:fix": "prettier --write --ignore-unknown .",
		"hint": "wait-on -t 300000 http://localhost:8080/ && hint http://localhost:8080/",
		"test": "concurrently 'npm:test:*'",
		"test:hint": "concurrently \"npm:start:prod\" \"npm:hint\" --kill-others  --s first",
		"release": "semantic-release",
		"prepare": "husky install",
		"git:pre-commit": "pretty-quick --staged && lint-staged",
		"git:pre-push": "npm run build && npm run test"
	},
	"dependencies": {
		"@fontsource/titillium-web": "^4.5.9"
	}
}

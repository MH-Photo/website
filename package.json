{
	"name": "mh-photography.com",
	"type": "module",
	"version": "3.0.0",
	"license": "MIT",
	"private": true,
	"description": "MH Photography",
	"keywords": ["website", "photography", "personal-website", "portfolio-website"],
	"repository": {
		"type": "git",
		"url": "https://github.com/MH-Photo/website.git"
	},
	"homepage": "https://mh-photography.com",
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=8.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.26.0",
		"@babel/preset-env": "^7.26.0",
		"@biomejs/biome": "^1.9.4",
		"@commitlint/cli": "^19.6.1",
		"@commitlint/config-conventional": "^19.6.0",
		"@csstools/postcss-oklab-function": "^4.0.6",
		"@hint/formatter-codeframe": "^3.1.36",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@typescript-eslint/eslint-plugin": "^8.18.1",
		"@typescript-eslint/parser": "^8.18.1",
		"autoprefixer": "^10.4.20",
		"babel-loader": "^9.2.1",
		"browserslist": "^4.24.3",
		"concurrently": "^9.1.0",
		"cssnano": "^7.0.6",
		"cssremedy": "^0.1.0-beta.2",
		"hint": "^7.1.13",
		"husky": "^9.1.7",
		"image-size": "^1.1.1",
		"postcss": "^8.4.49",
		"postcss-loader": "^8.1.1",
		"postcss-nesting": "^13.0.0",
		"postcss-preset-env": "^10.1.1",
		"prettier": "^3.3.3",
		"semantic-release": "^24.2.0",
		"sharp": "^0.33.5",
		"typescript": "^5.6.3",
		"vite": "^5.4.11",
		"wait-on": "^8.0.1"
	},
	"optionalDependencies": {
		"lint-staged": "^15.2.11"
	},
	"scripts": {
		"start": "astro dev",
		"start:prod": "astro preview",
		"build": "astro build",
		"build:img": "rm -rf public/assets/img && node bin/sharp.cjs",
		"update-portfolio": "rm -rf portfolio && node bin/onedrive.js --link \"$PORTFOLIO_URL\" --output portfolio",
		"lint": "prettier --check --ignore-unknown --no-error-on-unmatched-pattern . && biome ci .",
		"lint:fix": "prettier --write --ignore-unknown --no-error-on-unmatched-pattern .&& biome check . --write",
		"hint": "wait-on -t 300000 http://localhost:4321/ && hint http://localhost:4321/",
		"test": "concurrently 'npm:test:*'",
		"test:hint": "concurrently \"npm:start:prod\" \"npm:hint\" --kill-others  --s first",
		"release": "semantic-release",
		"postversion": "npm run lint:fix",
		"prepare": "husky",
		"git:pre-commit": "lint-staged",
		"git:pre-push": "npm run build && npm run test"
	},
	"dependencies": {
		"@astrojs/sitemap": "^3.2.1",
		"@fontsource/caveat": "^5.1.0",
		"@fontsource/gfs-didot": "^5.1.0",
		"@fontsource/titillium-web": "^5.1.0",
		"astro": "^5.0.9"
	}
}
